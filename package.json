{
  "name": "graphql-workshop",
  "description": "Workshop presented at JS@PayPal Spring 2018 where we build a GraphQL API w/ Node, Express and kraken-js",
  "version": "1.0.0",
  "private": true,
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/mstuart/graphql-workshop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstuart/graphql-workshop.git"
  },
  "bugs": {
    "url": "https://github.com/mstuart/graphql-workshop/issues"
  },
  "engines": {
    "node": ">=8"
